{
  "title": "Procedural Painter",
  "name": "procedural-painter",
  "version": "1.0.0",
  "description": "Procedurally generated impressionist paintings",
  "main": "./src/js/index.js",
  "scripts": {
    "build.dev": "webpack --config webpack.dev.js --progress",
    "build": "webpack --config webpack.prod.js --progress",
    "start": "webpack-dev-server --config webpack.dev.js --open",
    "deploy": "git push origin :gh-pages && git subtree push --prefix dist origin gh-pages",
    "watch": "webpack --config webpack.dev.js --progress --watch",
    "update:packages": "node wipe-dependencies.js && rm -rf node_modules && npm update --save-dev && npm update --save"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhudiaxyz/procedural-painter.git"
  },
  "keywords": [
    "procedural",
    "painting",
    "impressionist",
    "perlin",
    "noise",
    "generated",
    "computer",
    "graphics",
    "opengl",
    "webgl",
    "threejs",
    "three",
    "webpack",
    "webpack2",
    "es6",
    "babel",
    "sass"
  ],
  "author": "Bhupendra Bhudia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bhudiaxyz/procedural-painter/issues"
  },
  "homepage": "https://github.com/bhudiaxyz/procedural-painter",
  "dependencies": {
    "dat.gui": "^0.7.0",
    "orbit-controls-es6": "^1.0.10",
    "three": "^0.85.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "compression-webpack-plugin": "^1.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.29.0",
    "image-webpack-loader": "^3.3.1",
    "node-sass": "^4.5.3",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-glsl-loader": "^1.0.1",
    "webpack-merge": "^4.1.1"
  }
}
